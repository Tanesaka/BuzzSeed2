<% if users.blank? %>
<h5 class="mt-3">ユーザーはいません。<h3>
  <% else %>

      <table>
  <thread>
    <tr>
      <th>ニックネーム</th>
      <th>BuzzSeed ID</th>
    </tr>
  </thread>
<tbody>
<% users.each do |user| %>
    <tr>
       <td>
         <%= link_to user_path(user.id) do %>
         <%= user.name %>
         <% end %>
        </td>
      <td><%= user.unique_code %></td>
    </tr>
    <% end %>
  </tbody>
</table>


<% end %>